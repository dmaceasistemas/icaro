	<queryString><![CDATA[SELECT SUM(tbl_sas_detalle_ordenpago.monto), COUNT(tbl_sas_detalle_ordenpago.monto),
	extract(month from tbl_sas_detalle_ordenpago.fecha_ingreso), tbl_sas_ordenpago.estatus,
	tbl_sas_sede.codigo_sede, tbl_sas_sede.nombre,
	tbl_sas_empleado.condicion,
	CASE extract(month from tbl_sas_detalle_ordenpago.fecha_ingreso)
	  WHEN 1 THEN 'Enero'
	  WHEN 2 THEN 'Febrero'
	  WHEN 3 THEN 'Marzo'
	  WHEN 4 THEN 'Abril'
	  WHEN 5 THEN 'Mayo'
	  WHEN 6 THEN 'Junio'
	  WHEN 7 THEN 'Julio'
	  WHEN 8 THEN 'Agosto'
	  WHEN 9 THEN 'Septiembre'
	  WHEN 10 THEN 'Octubre'
	  WHEN 11 THEN 'Noviembre'
	  WHEN 12 THEN 'Diciembre'
	END AS mes_texto,
	CASE tbl_sas_empleado.condicion
	  WHEN 'O' THEN 'Obrero'
	  WHEN 'E' THEN 'Empleado'
	END AS condicion_texto
	FROM tbl_sas_detalle_ordenpago
	JOIN tbl_sas_ordenpago ON tbl_sas_detalle_ordenpago.numero_orden=tbl_sas_ordenpago.numero_orden
	JOIN tbl_sas_cobertura ON tbl_sas_ordenpago.numero_cobertura=tbl_sas_cobertura.numero_cobertura
	JOIN tbl_sas_empleado ON tbl_sas_cobertura.cedula_empleado=tbl_sas_empleado.cedula_empleado
	JOIN tbl_sas_sede ON tbl_sas_empleado.codigo_sede=tbl_sas_sede.codigo_sede
	GROUP BY 3, 4, tbl_sas_sede.codigo_sede, tbl_sas_sede.nombre, tbl_sas_empleado.condicion
	ORDER BY 3 DESC]]></queryString>

